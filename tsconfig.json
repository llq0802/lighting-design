{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "lighting-design": ["src", "docs"],
      "lighting-design/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "docs/**/*"]
}
